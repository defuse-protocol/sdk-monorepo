	},
	"files": [
		"dist",
		"schemas"
	],
	"scripts": {
		"build": "tsdown",
		"dev": "tsdown --watch",
		"lint": "biome check .",
		"format": "biome format --write .",
		"typecheck": "tsc --noEmit"
	},
	"devDependencies": {
		"ajv": "^8.17.1",
		"tsdown": "0.19.0",
		"typescript": "^5.4.2"
	}
