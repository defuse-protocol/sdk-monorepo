        "lint": "biome check .",
        "lint:fix": "biome check --write .",
        "format": "biome format --write .",
        "typecheck": "tsc --noEmit"
    },
    "devDependencies": {
        "msw": "^2.11.3",
        "tsdown": "0.15.5",
        "typescript": "^5.4.2",
        "vitest": "2.1.2"
    },
    "dependencies": {
        "@noble/secp256k1": "^3.0.0",
        "cbor2": "^2.0.1",
        "viem": "^2.31.7"
