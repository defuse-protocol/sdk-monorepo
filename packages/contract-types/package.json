{
	"name": "@defuse-protocol/contract-types",
	"version": "0.4.3",
	"private": false,
	"type": "module",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/defuse-protocol/sdk-monorepo.git",
		"directory": "packages/contract-types"
	},
	"bugs": {
		"url": "https://github.com/defuse-protocol/sdk-monorepo/issues"
	},
	"homepage": "https://github.com/defuse-protocol/sdk-monorepo/tree/main/packages/contract-types#readme",
	"sideEffects": false,
	"main": "./dist/index.cjs",
	"module": "./dist/index.js",
	"types": "./dist/index.d.ts",
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"import": "./dist/index.js",
			"require": "./dist/index.cjs",
			"default": "./dist/index.js"
		},
		"./schemas/intents.schema.json": "./schemas/intents.schema.json"
	},
	"files": [
		"dist",
		"schemas"
	],
	"scripts": {
		"build": "tsdown",
		"dev": "tsdown --watch",
		"lint": "biome check .",
		"lint:fix": "biome check --write .",
		"format": "biome format --write .",
		"typecheck": "tsc --noEmit"
	},
	"devDependencies": {
		"tsdown": "0.15.5",
		"typescript": "^5.4.2"
	}
}
